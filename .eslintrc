{
  "env": {
    "browser" : true,
    "node" : true,
    "es6": true
  },
  "extends": "eslint-config-google",
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off",
    "require-jsdoc": "off",
    "quotes": "off",
  }
}
